<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.chinashb.www.mobileerp.widget.CustomRecyclerView
        android:id="@+id/fragment_task_recyclerView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:listitem="@layout/task_listview_task_layout">

    </com.chinashb.www.mobileerp.widget.CustomRecyclerView>
    <com.chinashb.www.mobileerp.widget.EmptyLayoutManageView
        android:layout_width="match_parent"
        app:app_empty_text="没有数据"
        android:visibility="gone"
        app:app_empty_image_src="@mipmap/ic_empty_no_data"
        android:id="@+id/fragment_task_emptyManager"
        android:layout_height="match_parent"/>

</RelativeLayout>

