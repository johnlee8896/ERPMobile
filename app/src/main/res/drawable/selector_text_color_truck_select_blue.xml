<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:color="@color/color_333333" android:state_selected="false">

        <shape>
            <stroke android:width="@dimen/divide_line_size" android:color="@color/color_ccc" />
            <corners android:radius="@dimen/dimen_size_20" />
        </shape>
    </item>

    <item android:color="@color/color_blue_3D94FF" android:state_selected="true">
        <shape>
            <stroke android:width="@dimen/divide_line_size" android:color="@color/color_blue_3D94FF" />
            <corners android:radius="@dimen/dimen_size_20" />
        </shape>
    </item>


</selector>